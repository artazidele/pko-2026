<?xml version="1.0" encoding="UTF-8"?>
<plannerBenchmark xmlns="https://timefold.ai/xsd/benchmark" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://timefold.ai/xsd/benchmark https://timefold.ai/xsd/benchmark/benchmark.xsd">
    <benchmarkDirectory>local/benchmarkReport</benchmarkDirectory>

    <inheritedSolverBenchmark>
        <solver>
            <environmentMode>REPRODUCIBLE</environmentMode>
            <!-- Define the model -->
            <solutionClass>org.example.pko2.domain.Solution</solutionClass>
            <entityClass>org.example.pko2.domain.Visit</entityClass>
            <entityClass>org.example.pko2.domain.Car</entityClass>

            <!-- Define the score function -->
            <scoreDirectorFactory>
                <constraintProviderClass>org.example.pko2.solver.SolutionScoreCalculator</constraintProviderClass>
            </scoreDirectorFactory>

            <!-- Configure the optimization algorithms (optional) -->
            <termination>
                <secondsSpentLimit>10</secondsSpentLimit>
            </termination>

            <constructionHeuristic />
        </solver>
        <problemBenchmarks>
            <solutionFileIOClass>org.example.pko2.domain.SolutionJsonIO</solutionFileIOClass>
            <writeOutputSolutionEnabled>true</writeOutputSolutionEnabled>
            <inputSolutionFile>local/data/example-data-1.json</inputSolutionFile>
            <inputSolutionFile>local/data/example-data-2.json</inputSolutionFile>
            <inputSolutionFile>local/data/example-data-3.json</inputSolutionFile>
            <inputSolutionFile>local/data/example-data-4.json</inputSolutionFile>
            <inputSolutionFile>local/data/example-data-5.json</inputSolutionFile>
        </problemBenchmarks>
    </inheritedSolverBenchmark>

    <solverBenchmark>
        <name>Hill Climbing</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <acceptorType>HILL_CLIMBING</acceptorType>
                </acceptor>
                <forager>
                    <acceptedCountLimit>1</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Tabu Search 500 - 1000</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <entityTabuSize>500</entityTabuSize>
                </acceptor>
                <forager>
                    <acceptedCountLimit>1000</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Tabu Search 500 - 5000</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <entityTabuSize>500</entityTabuSize>
                </acceptor>
                <forager>
                    <acceptedCountLimit>5000</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Tabu Search 1000 - 5000</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <entityTabuSize>1000</entityTabuSize>
                </acceptor>
                <forager>
                    <acceptedCountLimit>5000</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Simulated Annealing 0-1000</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <simulatedAnnealingStartingTemperature>0hard/1000soft</simulatedAnnealingStartingTemperature>
                </acceptor>
                <forager>
                    <acceptedCountLimit>1</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Simulated Annealing 1-5000</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <simulatedAnnealingStartingTemperature>1hard/5000soft</simulatedAnnealingStartingTemperature>
                </acceptor>
                <forager>
                    <acceptedCountLimit>1</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Simulated Annealing 2-10000</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <simulatedAnnealingStartingTemperature>2hard/10000soft</simulatedAnnealingStartingTemperature>
                </acceptor>
                <forager>
                    <acceptedCountLimit>1</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Late Acceptance 200</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <lateAcceptanceSize>200</lateAcceptanceSize>
                </acceptor>
                <forager>
                    <acceptedCountLimit>1</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Late Acceptance 400</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <lateAcceptanceSize>400</lateAcceptanceSize>
                </acceptor>
                <forager>
                    <acceptedCountLimit>1</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
    <solverBenchmark>
        <name>Late Acceptance 600</name>
        <solver>
            <localSearch>
                <unionMoveSelector>
                    <subListChangeMoveSelector />
                    <listChangeMoveSelector />
                    <listSwapMoveSelector />
                </unionMoveSelector>
                <acceptor>
                    <lateAcceptanceSize>600</lateAcceptanceSize>
                </acceptor>
                <forager>
                    <acceptedCountLimit>1</acceptedCountLimit>
                </forager>
            </localSearch>
        </solver>
    </solverBenchmark>
</plannerBenchmark>